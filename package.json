  },
  "description": "Protractor end-to-end test automation boilerplate. Using TypeScript, ts-node, Mocha and Assert.",
  "devDependencies": {
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.4",
    "husky": "4.2.3",
    "lint-staged": "10.0.9",
    "prettier": "2.0.2",
    "tslint": "6.1.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.2.0"
  },
  "homepage": "https://github.com/e2e-boilerplate/protractor-typescript-ts-node-mocha-assert#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "assert",
    "boilerplate",
    "e2e",
    "e2e tests",
    "example",
    "integration test",
    "mocha",
    "protractor",
    "ts-node",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ]
  },
  "name": "protractor-typescript-ts-node-mocha-assert",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e2e-boilerplate/protractor-typescript-ts-node-mocha-assert.git"
  },
  "scripts": {
    "lint": "npx tslint -c tslint.json './spec/*.ts'",
    "pretty": "npx prettier --no-config './spec/*.ts' --write",
