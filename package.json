{
  "author": "Girma Nigusse <xgirma@gmail.com>",
  "bugs": {
    "url": "https://github.com/e2e-boilerplate/protractor-typescript-ts-node-mocha-assert/issues"
  },
  "dependencies": {
    "mocha": "7.1.1",
